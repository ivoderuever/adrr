{
  "manifest_version": 3,
  "name": "Adrr",
  "version": "1.0.0",
  "description": "An extension to set address data with one click",
  "icons": {
    "16": "icon/logo-16.png",
    "32": "icon/logo-32.png",
    "48": "icon/logo-48.png",
    "128": "icon/logo-128.png"
  },
  "action": {
    "default_popup": "ui.html"
  },
  "host_permissions": [
    "https://*.custtom.com/*",
    "https://*.custtom.nl/*",
    "https://*.custtom.de/*",
    "https://*.custtom.fr/*",
    "https://*.custtom.co.uk/*",
    "https://*.custtom.ie/*"
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
}